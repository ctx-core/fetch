{
	"name": "@ctx-core/fetch",
	"version": "11.0.102",
	"description": "ctx-core fetch",
	"keywords": [
		"ctx-core",
		"fetch"
	],
	"homepage": "https://github.com/ctx-core/fetch#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/fetch/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/fetch.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./lib/index.d.ts",
	"exports": {
		".": {
			"browser": "./lib/index.browser.js",
			"node": "./lib/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf lib && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rm -f tsconfig.tsbuildinfo && rm -f lib/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir lib --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir lib",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/app_version": "workspace:^15.0.5",
		"@ctx-core/dom": "workspace:^11.1.0",
		"@ctx-core/error": "workspace:^11.1.17",
		"@ctx-core/fibonacci": "workspace:^8.0.33",
		"@ctx-core/object": "workspace:^22.0.8",
		"@ctx-core/package": "workspace:^9.1.20",
		"@ctx-core/sleep": "workspace:^8.0.32",
		"node-fetch": "next"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.124",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./lib/index.js"
}