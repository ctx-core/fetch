		"@ctx-core/object": "workspace:^20.2.5",
		"@ctx-core/package": "workspace:^8.0.15",
		"@ctx-core/sleep": "workspace:^7.0.10",
		"isomorphic-fetch": "^3.0.0",
		"whatwg-fetch": "^3.6.2"
	},
	"devDependencies": {
		"typescript": "^4.3.5"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
