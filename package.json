{
	"name": "@ctx-core/fetch",
	"version": "11.0.99",
	"description": "ctx-core fetch",
	"keywords": [
		"ctx-core",
		"fetch"
	],
	"homepage": "https://github.com/ctx-core/fetch#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/fetch/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/fetch.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"browser": "./dist/index.browser.js",
			"node": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist && npm run clean_tsbuildinfo",
		"clean_tsbuildinfo": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.d.ts",
		"compile": "npm run compile_source && npm run compile_declaration",
		"compile_source": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"compile_declaration": "npm run clean_tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir dist",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/app_version": "workspace:^14.0.7",
		"@ctx-core/dom": "workspace:^11.0.46",
		"@ctx-core/error": "workspace:^11.1.15",
		"@ctx-core/fibonacci": "workspace:^8.0.31",
		"@ctx-core/object": "workspace:^22.0.6",
		"@ctx-core/package": "workspace:^9.1.18",
		"@ctx-core/sleep": "workspace:^8.0.30",
		"node-fetch": "next"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./dist/index.js"
}
