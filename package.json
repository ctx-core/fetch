{
	"name": "@ctx-core/fetch",
	"version": "10.0.0",
	"description": "ctx-core fetch",
	"keywords": [
		"ctx-core",
		"fetch"
	],
	"homepage": "https://github.com/ctx-core/fetch#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/fetch/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/fetch/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./src/index.js",
	"module": "./src/index.js",
	"types": "./src/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.js && rm -f dist/**/*.d.ts && rm -f dist/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@",
		"prepublishOnly": "npm run compile"
	},
	"dependencies": {
		"@ctx-core/app_version": "workspace:^11.0.1",
		"@ctx-core/dom": "workspace:^10.0.1",
		"@ctx-core/error": "workspace:^10.0.0",
		"@ctx-core/fibonacci": "workspace:^7.0.0",
		"@ctx-core/object": "workspace:^20.0.1",
		"@ctx-core/package": "workspace:^8.0.0",
		"@ctx-core/sleep": "workspace:^7.0.0",
		"isomorphic-fetch": "^3.0.0",
		"whatwg-fetch": "^3.6.2"
	},
	"devDependencies": {
		"typescript": "^4.3.2"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
